{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-param-reassign":"off",
        "class-methods-use-this":"off",
        "no-restricted-syntax":"off",
        "radix":"off",
        "no-plusplus":"off",
        "eqeqeq":"off",
        "no-useless-escape":"off",
        "no-undef":"off",
        "no-useless-constructor":"off",
        "import/extensions":"off",
        "import/no-unresolved":"off"
    }
}
